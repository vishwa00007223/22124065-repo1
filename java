public class SumOfNumbers {
    public static void main(String[] args) {
        int sum = 0;
        
        // Loop to calculate the sum of first 10 natural numbers
        for (int i = 1; i <= 10; i++) {
            sum += i;
        }
        
        // Print the result
        System.out.println("The sum of the first 10 natural numbers is: " + sum);
    }
}

